def sort(list):
    for i in range(len(list)-1):
        minpos= i
        for j in range(i,len(list)):
            if list[minpos] > list[j]:
                minpos = j
             
                
        temp = list[i]
        list[i] = list[minpos]
        list[minpos]= temp
       

a =  [5,1,0,9,4,8,3]
sort(a)
print(a)
